function IsAllowLogicalFlag(allStageID)

{

	if (allStageID < 4)

	{

		let tmpLap = 0;

		let tmpStage = allStageID;

		return(CDM_GetPracticeStageFlag(GetPlayerType(), tmpStage, 0, tmpLap));

	}

	else if (allStageID < 8)

	{

		let tmpLap = 1;

		let tmpStage = allStageID - 4;

		return(CDM_GetPracticeStageFlag(GetPlayerType(), tmpStage, 0, tmpLap));

	}

	else if (allStageID < 10)

	{

		let tmpLap = [0, 1][allStageID == 9];

		return(CDM_GetPracticeStageFlag(GetPlayerType(), STAGE_ENDING, 0, tmpLap));

	}

	else if (allStageID < 11)

	{

		let tmpLap = 0;

		let tmpStage = 7;

		return(CDM_GetPracticeStageFlag(GetPlayerType(), tmpStage, 0, tmpLap));

	}

	return(false);

}



