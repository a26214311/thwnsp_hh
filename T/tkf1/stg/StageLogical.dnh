// 鬮｣雋ｻ�ｽ�ｨ髯樊ｺｷ�･�ｽ�ｽ�ｽ�ｽ�ｩ�ｽ�ｽ�ｽ�ｱ鬯ｨ�ｾ陋ｹ�ｽ�ｽ�ｽ�ｽ�ｨ

function CallTalkScene(path)

{

	// 鬯ｯ�ｨ�ｽ�ｾ�ｽ�ｽ�ｽ�｣鬯ｩ謳ｾ�ｽ�ｯ髯橸ｽ｢�ｽ�ｼ�ｽ�ｽ�ｽ�ｽ鬯ｨ�ｾ陋ｹ�ｽ�ｽ�ｿ�ｽ�ｽ

	let objBossScene = ObjEnemyBossScene_Create();

	ObjEnemyBossScene_Add(objBossScene, 0, path);

	ObjEnemyBossScene_LoadInThread(objBossScene);

	

	descent(t in 0 .. 30)

	{

		MENU_ALPHA = 255 * t/30;

		yield;

	}

	wait(30);



	// 鬩幢ｽ｢隴趣ｿｽ�ｿ�ｽ驍ｵ�ｺ陝ｶ�ｷ�ｽ�ｹ�ｽ�ｧ�ｽ�ｽ�ｽ�ｷ鬩幢ｽ｢隴趣ｽ｢�ｽ�ｽ�ｽ�ｼ鬩幢ｽ｢隴趣ｽ｢�ｽ�ｽ�ｽ�ｳ鬯ｯ�ｮ�ｽ�｢髯ｷ�ｿ�ｽ�･�ｽ�ｽ�ｽ�ｧ�ｽ�ｽ�ｽ�ｽ

	ObjEnemyBossScene_Regist(objBossScene);



	//鬮ｫ�ｰ�ｽ�ｨ�ｽ�ｽ�ｽ�ｵ鬩幢ｽ｢隴趣ｿｽ�ｿ�ｽ驍ｵ�ｺ陝ｶ�ｷ�ｽ�ｹ�ｽ�ｧ�ｽ�ｽ�ｽ�ｷ鬩幢ｽ｢隴趣ｽ｢�ｽ�ｽ�ｽ�ｼ鬩幢ｽ｢隴趣ｽ｢�ｽ�ｽ�ｽ�ｳ鬩搾ｽｵ�ｽ�ｺ髫ｶ蜻ｵ�ｶ�｣�ｽ�ｽ�ｽ�ｵ驛｢�ｧ�ｽ�ｽ�ｽ�ｽ�ｽ�ｺ�ｽ�ｽ�ｽ�ｽ髫ｨ蛛�ｽｿ�ｽ�ｽ�ｹ�ｽ�ｧ髣包ｽｵ隴擾ｽｶ驕ｨ螳｣縺暦ｿｽ�ｺ�ｽ�ｽ�ｽ�ｧ鬮ｯ貅ｷ譯�ｿｽ�ｿ�ｽ�ｽ�ｽ�ｽ�ｽ�ｩ�ｽ�ｽ�ｽ�ｽ

	while(!Obj_IsDeleted(objBossScene))

	{

		yield;

	}



	ascent(t in 0 .. 15)

	{

		MENU_ALPHA = 255 * (t+1)/15;

		yield;

	}

}

// ED鬯ｨ�ｾ陋ｹ�ｽ�ｽ�ｽ�ｽ�ｨ

function CallEndingScene(path)

{

	let idScript = LoadScript(path);

	StartScript(idScript);



	while(!IsCloseScript(idScript)) { yield; }

}





//鬩幢ｽ｢隴趣ｽ｢�ｽ�ｽ�ｽ�｡鬩幢ｽ｢隴乗��ｽ�ｹ隴∵ｺｽ�､�ｼ�ｽ�ｹ隴趣ｽ｢�ｽ�ｽ�ｽ�ｼ鬯ｩ蜍溪��ｽ�ｽ�ｽ�ｻ鬮ｯ�ｷ髢ｧ�ｴ�ｽ�ｽ鬨ｾ�｡�ｽ�ｽ

task CheckMenuOffset(parent, offset_point)

{

	let to_y = MENU_OFFSET;

	while(!Obj_IsDeleted(parent))

	{

		let selectY = offset_point[Obj_GetValueD(parent, "VALUE", 0)];

		if (selectY > 256) { to_y = 256-selectY; }

		else { to_y = 0; }

		MENU_OFFSET = to_y*0.25 + MENU_OFFSET*0.75;

		yield;

	}

}



//鬩幢ｽ｢�ｽ�ｧ�ｽ�ｽ�ｽ�ｿ鬩幢ｽ｢�ｽ�ｧ�ｽ�ｽ�ｽ�､鬩幢ｽ｢隴主�讓滂ｿｽ蜿冶ｾｨ�ｽ�ｬ陋幢ｿｽ�ｽ�ｿ�ｽ�ｽ

task SetStageTitle(parent, numStage)

{

	if (numStage < 0) { return; }

	

	let imgX = 430;

	let imgY = 64;

	

	let obj1 = CreateLogicalText(imgX, imgY, TITLE_TEXT[numStage*3+0]);

	Obj_SetRenderPriorityI(obj1, 90);

	ObjText_SetFontBold(obj1, true);

	

	let obj2 = CreateLogicalText(imgX-50, imgY+24, TITLE_TEXT[numStage*3+1]);

	Obj_SetRenderPriorityI(obj2, 90);

	ObjText_SetMaxWidth(obj2, 300);

	ObjText_SetHorizontalAlignment(obj2, ALIGNMENT_CENTER);

	ObjText_SetFontBold(obj2, true);



	let obj3 = CreateLogicalText(imgX, imgY+56, TITLE_TEXT[numStage*3+2]);

	Obj_SetRenderPriorityI(obj3, 90);

	ObjText_SetMaxWidth(obj3, 200);

	ObjText_SetFontSize(obj3, 22);



	let flg = Obj_GetValueD(parent, "VALUE", -1);



	while(!Obj_IsDeleted(parent) && flg == Obj_GetValueD(parent, "VALUE", -1))

	{

		yield;

	}

	Obj_Delete(obj1);

	Obj_Delete(obj2);

	Obj_Delete(obj3);

}



//鬮ｫ�ｴ�ｽ�ｽ�ｽ�ｿ�ｽ�ｽ�ｽ�ｽ�ｽ�ｭ髯ｷ莨夲ｽｽ�ｱ�ｽ�ｽ陝ｶ譌�ｿｽ�ｿ�ｽ�ｽ�ｮ鬩搾ｽｵ�ｽ�ｺ髣包ｽｳ陝ｯ�ｩ陷ｻ�ｳ鬩搾ｽｵ�ｽ�ｺ�ｽ�ｽ�ｽ�ｽ

task TMenuInfo(parent, mx, my, text, font_size)

{

	let objText = CreateLogicalText(mx, my, text);

	ObjText_SetFontSize(objText, font_size);

	descent(t in 0 .. 10)

	{

		let offset_y = -t^2;

		let alpha = 1 * (10-t)/10;

		ObjRender_SetX(objText, mx+offset_y);

		ObjRender_SetAlpha(objText, MENU_ALPHA*alpha);

		yield;

	}

	while(!Obj_IsDeleted(parent))

	{

		ObjRender_SetY(objText, my+MENU_OFFSET);

		ObjRender_SetAlpha(objText, MENU_ALPHA);

		yield;

	}

	Obj_Delete(objText);

}



//鬩幢ｽ｢隴趣ｽ｢�ｽ�ｽ�ｽ�｡鬩幢ｽ｢隴乗��ｽ�ｹ隴∵ｺｽ�､�ｼ�ｽ�ｹ隴趣ｽ｢�ｽ�ｽ�ｽ�ｼ鬯ｮ�ｫ髯ｬ諛医′�ｽ�ｽ�ｽ�ｴ�ｽ�ｽ�ｽ�ｽ

task TMenuItem(parent, index, mx, my, text)

{

	let objText = CreateLogicalText(mx, my, text);

	ObjRender_SetColor(objText, 128, 128, 128);

	

	descent(t in 0 .. 10)

	{

		let offset_x = t^2;

		let alpha = 1 * (10-t)/10;

		ObjRender_SetX(objText, mx+offset_x);

		ObjRender_SetAlpha(objText, MENU_ALPHA*alpha);

		yield;

	}

	let selected = false;

	while(!Obj_IsDeleted(parent))

	{

		if (index == Obj_GetValueD(parent, "VALUE", -1))

		{

			ObjRender_SetColor(objText, 255, 255, 255);

		} else {

			ObjRender_SetColor(objText, 96, 96, 96);

		}

		ObjRender_SetY(objText, my+MENU_OFFSET);

		ObjRender_SetAlpha(objText, MENU_ALPHA);

		yield;

	}

	Obj_Delete(objText);

}



// 鬩幢ｽ｢隴趣ｽ｢�ｽ�ｿ�ｽ�ｽ驍ｵ�ｺ陷�圜�ｽ�ｹ�ｽ�ｧ�ｽ�ｽ�ｽ�ｹ鬩幢ｽ｢隴主�讓滄し�ｺ隶呵ｶ｣�ｽ�ｹ隴趣ｿｽﾂｧ驍ｵ�ｺ陞溘ｑ�ｽ�ｹ�ｽ�ｧ�ｽ�ｽ�ｽ�ｧ鬩幢ｽ｢�ｽ�ｧ�ｽ�ｽ�ｽ�ｯ鬩幢ｽ｢隴主�讓滂ｿｽ�ｽ陞ｳ螢ｽ蜑ｲ髫ｲ蟶幢ｽ･�ｽ�ｽ�ｽ鬯倩ｲｻ�ｽ�ｸ�ｽ�ｺ�ｽ�ｽ�ｽ�ｾ鬩搾ｽｵ�ｽ�ｺ髯ｷ�ｷ�ｽ�ｶ髫ｨ�ｳ�ｽ�ｽ�ｽ�ｿ�ｽ�ｽ髫ｴ莨夲ｽｽ�ｦ�ｽ�ｽ�ｽ�ｽ髫ｴ莨夲ｽｽ�ｦ�ｽ�ｽ�ｽ�ｽ�ｽ�ｽ�ｽ�ｽ

function CreateLogicalText(x, y, text)

{

	// 鬩幢ｽ｢隴趣ｽ｢�ｽ�ｿ�ｽ�ｽ驍ｵ�ｺ陷�圜�ｽ�ｹ�ｽ�ｧ�ｽ�ｽ�ｽ�ｹ鬩幢ｽ｢隴乗��ｽ�ｺ陋ｹ�ｽ�ｽ�ｽ�ｽ�ｽ髫ｲ蟶幢ｽ･�ｽ�ｽ�ｽ霑｢證ｦ�ｽ�ｹ�ｽ�ｧ鬮｣魃会ｽｽ�ｨ�ｽ�ｽ�ｽ�ｼ�ｽ�ｽ�ｽ�ｽ

	let obj = ObjText_Create();

	Obj_SetRenderPriorityI(obj, 70);

	ObjText_SetFontType(obj, "Meiryo");

	ObjText_SetFontSize(obj, 24);

	ObjText_SetLinePitch(obj, -1);

	ObjText_SetFontBorderWidth(obj, 1);

	ObjText_SetFontBorderColor(obj, 32, 32, 32);

	ObjText_SetFontBorderType(obj, BORDER_SHADOW);

	ObjText_SetText(obj, text);

	ObjRender_SetPosition(obj, x, y, 0);

	return(obj);



}



